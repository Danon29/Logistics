@model Logistics.Models.Data.Sender

@{
    ViewBag.Title = "Добавление Отправителя";
}

<form asp-action="AddOrderSenderPost" asp-controller="Data">
    
    <div class="form-group">
        <label asp-for="Name" class="control-label">Имя</label>
        <input type="text" asp-for="Name" class="form-controll" />
    </div>

    <div class="form-group">
        <label asp-for="Surname" class="control-label">Фамилия</label>
        <input type="text" asp-for="Surname" class="form-controll" />
    </div>

    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>

</form>

